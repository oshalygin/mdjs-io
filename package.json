{
  "name": "merchant-dashboard",
  "version": "1.0.0",
  "description": "Merchant Dashboard built with React and Redux",
  "scripts": {
    "prestart": "babel-node utilities/startingDisplayMessage.js",
    "start": "npm-run-all --parallel open:server",
    "windows-start": "npm-run-all --parallel open-windows:server",
    "open:server": "babel-watch ./server/application.js",
    "open-windows:server": "babel-node ./server/application.js",
    "lint": "./node_modules/.bin/eslint -c .eslintrc webpack.config.* client/**/*.js client/**/*.jsx server/**/*.js test ./utilities --color",
    "lint:watch": "npm run lint -- --watch",
    "test:watch": "npm run tests -- --watch",
    "tests": "NODE_ENV=test mocha --reporter min",
    "windows-tests": "SET NODE_ENV=test && mocha --reporter min",
    "test": "NODE_ENV=test babel-node ./node_modules/.bin/isparta cover --include 'client/**/*.js*' _mocha",
    "windows-test": "SET NODE_ENV=test && babel-node ./node_modules/isparta/bin/isparta cover --report html node_modules/mocha/ bin/_mocha -- --reporter spec",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node utilities/buildHtml.js",
    "prebuild": "npm-run-all clean-dist test lint build:html",
    "windows-prebuild": "npm-run-all clean-dist windows-test lint build:html windows-buildjs",
    "build": "babel-node utilities/build.js",
    "windows-buildjs": "babel-node utilities/build.js",
    "windows-build": "npm-run-all windows-prebuild postbuild",
    "postbuild": "babel-node ./server/application.production.js"
  },
  "author": "Oleg Shalygin",
  "license": "GNU AGPLv3",
  "dependencies": {
    "animate.css": "^3.5.2",
    "axios": "^0.14.0",
    "babel-polyfill": "^6.13.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "eslint": "^3.5.0",
    "font-awesome": "^4.6.3",
    "image-webpack-loader": "^2.0.0",
    "jquery": "^3.1.0",
    "jquery-slimscroll": "^1.3.8",
    "jsx-loader": "^0.13.2",
    "lodash": "^4.16.0",
    "material-design-lite": "^1.1.3",
    "metismenu": "^2.5.2",
    "pace": "0.0.4",
    "pace-progress": "^1.0.2",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "4.4.5",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "toastr": "^2.1.2",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "^6.14.0",
    "babel-watch": "^2.0.2",
    "cheerio": "^0.22.0",
    "colors": "1.1.2",
    "compression": "^1.6.2",
    "coveralls": "^2.11.14",
    "cross-env": "^2.0.1",
    "css-loader": "^0.25.0",
    "enzyme": "^2.4.1",
    "eslint": "^3.5.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-react": "^6.3.0",
    "eslint-watch": "^2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "^1.20.2",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "^0.9.0",
    "isparta": "^4.0.0",
    "istanbul": "^0.4.4",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.0.0",
    "mocha": "^3.0.2",
    "mocha-lcov-reporter": "^1.2.0",
    "mock-require": "^1.3.0",
    "nock": "8.0.0",
    "npm-run-all": "^3.1.0",
    "open": "0.0.5",
    "react-addons-test-utils": "^15.3.2",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-logger": "^2.6.1",
    "redux-mock-store": "^1.2.0",
    "rimraf": "^2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "wallaby-webpack": "0.0.23",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oshalygin/MerchantDashboard.git"
  }
}
