{
  "name": "mdjs",
  "version": "2.31.0",
  "description": "Merchant Dashboard built with React and Redux",
  "scripts": {
    "prestart": "babel-node utilities/startingDisplayMessage.js",
    "start": "npm-run-all --parallel open:server test:watch lint:watch",
    "open:dist": "node ./server/index.js",
    "open:server": "babel-watch ./server/application.dev.js",
    "lint": "esw webpack.config.* client server utilities --color --ext .js --ext .jsx",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest --silent",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "jest --coverage",
    "codacy:coverage": "cat ./coverage/lcov.info | codacy-coverage",
    "clean-dist": "npm run remove-dist && mkdir dist",
    "remove-dist": "node_modules/.bin/rimraf ./dist",
    "build:html": "babel-node utilities/buildHtml.js",
    "prebuild": "npm-run-all clean-dist build:html",
    "build": "babel-node utilities/build.js"
  },
  "author": "Oleg Shalygin",
  "license": "AGPL-3.0",
  "dependencies": {
    "@google-cloud/trace-agent": "1.0.1",
    "animate.css": "3.5.2",
    "axios": "0.15.3",
    "babel-polyfill": "6.23.0",
    "babel-runtime": "6.23.0",
    "body-parser": "1.16.1",
    "bootstrap": "3.3.7",
    "chalk": "1.1.3",
    "codacy-coverage": "2.0.1",
    "cookie-parser": "1.4.3",
    "dashboard-styles": "2.0.1",
    "date-fns": "1.28.4",
    "express": "4.14.1",
    "express-winston": "2.2.0",
    "file-loader": "0.10.1",
    "font-awesome": "4.6.3",
    "form-data": "2.1.2",
    "getmdl-select": "1.0.4",
    "hsts": "2.0.0",
    "image-webpack-loader": "3.2.0",
    "jquery": "3.1.0",
    "jsx-loader": "0.13.2",
    "lodash": "4.16.6",
    "material-design-lite": "1.1.3",
    "material-ui": "0.17.1",
    "metismenu": "2.6.3",
    "moment": "2.17.1",
    "multer": "1.3.0",
    "node-fetch": "1.6.3",
    "open": "0.0.5",
    "pace": "0.0.4",
    "pace-progress": "1.0.2",
    "react": "15.4.2",
    "react-css-modules": "4.1.0",
    "react-dom": "15.4.2",
    "react-dropzone": "3.10.0",
    "react-redux": "5.0.3",
    "react-router": "3.0.2",
    "react-router-redux": "4.0.8",
    "react-tap-event-plugin": "2.0.1",
    "recharts": "0.22.4",
    "redux": "3.6.0",
    "redux-devtools-extension": "2.13.0",
    "redux-thunk": "2.2.0",
    "request": "2.81.0",
    "request-promise": "4.2.0",
    "style-loader": "0.13.2",
    "url-loader": "0.5.8",
    "uuid": "3.0.1",
    "winston": "2.3.1",
    "winston-express": "0.1.1"
  },
  "devDependencies": {
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.3.2",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-rest-spread": "6.23.0",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.22.0",
    "babel-regenerator-runtime": "6.5.0",
    "babel-register": "6.23.0",
    "babel-template": "6.23.0",
    "babel-types": "6.23.0",
    "babel-watch": "2.0.6",
    "chai": "3.5.0",
    "cheerio": "0.22.0",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cross-env": "3.0.0",
    "css-loader": "0.26.2",
    "css-modules-require-hook": "4.0.5",
    "enzyme": "2.7.1",
    "eslint": "3.19.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jest": "19.0.1",
    "eslint-plugin-react": "6.10.0",
    "eslint-watch": "3.1.0",
    "eventsource-polyfill": "0.9.6",
    "extract-text-webpack-plugin": "1.0.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "19.0.2",
    "jsdom": "9.11.0",
    "jsdom-global": "2.0.0",
    "moxios": "0.3.0",
    "npm-run-all": "4.0.2",
    "postcss-cssnext": "2.9.0",
    "postcss-import": "9.1.0",
    "postcss-loader": "1.3.2",
    "react-addons-test-utils": "15.4.2",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-logger": "2.8.1",
    "redux-mock-store": "1.2.2",
    "resolve-url": "0.2.1",
    "rimraf": "2.6.1",
    "sinon": "1.17.7",
    "source-map-support": "0.4.14",
    "wallaby-webpack": "0.0.34",
    "webpack": "1.14.0",
    "webpack-dev-middleware": "1.10.1",
    "webpack-hot-middleware": "2.17.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oshalygin/mdjs.git"
  },
  "jest": {
    "testRegex": ".*?(\\.spec).js",
    "transformIgnorePatterns": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "client/**/*.js",
      "client/**/*.jsx",
      "server/**/*.js*"
    ],
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest/build/index.js"
    },
    "setupFiles": [
      "./utilities/testSetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/utilities/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "coverageReporters": [
      "json",
      "html",
      "text",
      "lcov",
      "clover"
    ]
  }
}
